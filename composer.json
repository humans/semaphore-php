{
    "name": "humans/laravel-semaphore",
    "description": "Semaphore integration to Laravel's notification layer.",
    "type": "library",
    "keywords": ["laravel", "sms", "semaphore", "notification"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jaggy Gauran",
            "email": "i.am@jag.gy"
        }
    ],
    "require": {
        "php": ">=7.0",
        "kitetail/zttp": "^0.4.0",
        "illuminate/support": "5.5|5.6|5.7|5.8|^6.0",
        "illuminate/notifications": "5.5|5.6|5.7|5.8|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Humans\\Semaphore\\": "src"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Humans\\Semaphore\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
